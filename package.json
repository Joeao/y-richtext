    "blanket": "^1.1.6",
    "coveralls": "^2.11.2",
    "gulp-notify": "^2.2.0",
    "gulp-util": "^3.0.4",
    "mocha-lcov-reporter": "0.0.2",
    "mocha-multi": "^0.6.0"
