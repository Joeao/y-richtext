{
  "name": "y-richtext",
  "version": "0.5.2",
  "description": "RichText Type for Yjs",
  "main": "./build/node/y-richtext.js",
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "scripts": {
    "test": "gulp mocha; gulp coveralls",
    "prepublish": "gulp build_node"
  },
  "config": {
    "blanket": {
      "loader": "./node-loaders/coffee-script",
      "pattern": "/lib/",
      "data-cover-never": [
        "node_modules",
        "test"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/y-js/y-richtext"
  },
  "keywords": [
    "Yjs",
    "xml",
    "ot",
    "collaboration",
    "synchronization",
    "sharejs",
    "coweb",
    "concurrency",
    "rich text"
  ],
  "author": "Kevin Jahns <kevin.jahns@rwth-aachen.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/y-js/y-richtext/issues"
  },
  "homepage": "http://y-js.org",
  "devDependencies": {
    "codo": "^2.0.9",
    "coffee-errors": "~0.8.6",
    "coffeeify": "^0.6.0",
    "gulp": "^3.8.7",
    "gulp-browserify": "^0.5.0",
    "gulp-cached": "^1.0.1",
    "gulp-coffee": "^2.1.1",
    "gulp-coffeeify": "^0.1.2",
    "gulp-coffeelint": "^0.3.3",
    "gulp-concat": "^2.5.2",
    "gulp-copy": "0.0.2",
    "gulp-coveralls": "^0.1.3",
    "gulp-debug": "^1.0.0",
    "gulp-exit": "0.0.2",
    "gulp-git": "^0.5.0",
    "gulp-if": "^1.2.4",
    "gulp-ignore": "^1.2.0",
    "gulp-jshint": "^1.9.2",
    "gulp-ljs": "^0.1.1",
    "gulp-mocha": "^0.5.2",
    "gulp-plumber": "^0.6.6",
    "gulp-rename": "^1.2.0",
    "gulp-rimraf": "^0.1.0",
    "gulp-run": "^1.6.3",
    "gulp-sass": "^1.3.3",
    "gulp-shell": "^0.4.0",
    "gulp-sourcemaps": "^1.1.1",
    "gulp-uglify": "^1.1.0",
    "gulp-watch": "^3.0.0",
